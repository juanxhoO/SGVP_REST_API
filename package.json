{
  "name": "prisma-express-typescript-boilerplate",
  "version": "1.0.0",
  "type": "commonjs",
  "description": "REST API Boilerplate with Node JS, TypeScript, Express and Prisma",
  "main": "src/index.ts",
  "repository": "https://github.com/juanxhoO/SGVP_REST_API.git",
  "scripts": {
    "start": "yarn build && pm2 start ecosystem.config.json --no-daemon",
    "dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "test": "docker-compose -f docker-compose.only-db-test.yml up -d && npm run db:push && jest -i --colors --verbose --detectOpenHandles && docker-compose -f docker-compose.only-db-test.yml down",
    "test:watch": "docker-compose -f docker-compose.only-db-test.yml up -d && yarn db:push && jest -i --watchAll && docker-compose -f docker-compose.only-db-test.yml down",
    "lint": "eslint .",
    "sonar": "sonarqube-scanner",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier --check **/*.ts",
    "prettier:fix": "prettier --write **/*.ts",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "docker:dev-db:start": "docker compose -f docker-compose.only-db-dev.yml up -d",
    "docker:dev-db:stop": "docker compose -f docker-compose.only-db-dev.yml down",
    "prepare": "husky install",
    "build": "rimraf build && tsc -p tsconfig.json"
  },
  "keywords": [
    "node.js",
    "typescript",
    "boilerplate",
    "express",
    "rest",
    "api",
    "prisma",
    "mysql",
    "es6",
    "es7",
    "es8",
    "es9",
    "docker",
    "passport",
    "joi",
    "eslint",
    "prettier"
  ],
  "author": "Juan Granja",
  "license": "ISC",
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.15",
    "@types/express": "^4.17.20",
    "@types/jest": "^29.5.6",
    "@types/morgan": "^1.9.7",
    "@types/node": "^20.8.7",
    "@types/passport": "^1.0.14",
    "@types/passport-jwt": "^3.0.11",
    "@types/supertest": "^2.0.15",
    "@types/swagger-jsdoc": "^6.0.2",
    "@types/swagger-ui-express": "^4.1.5",
    "@types/xss-filters": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^6.8.0",
    "@typescript-eslint/parser": "^6.8.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "node-mocks-http": "^1.12.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "prisma": "^5.4.2",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.4.2",
    "@types/bcryptjs": "^2.4.5",
    "@types/nodemailer": "^6.4.13",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.0.0",
    "helmet": "^7.0.0",
    "http-status": "^1.5.3",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.6",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pm2": "^5.3.0",
    "sonarqube-scanner": "^4.0.1",
    "winston": "^3.11.0",
    "xss-filters": "^1.2.7"
  }
}
